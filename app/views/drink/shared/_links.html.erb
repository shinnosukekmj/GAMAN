<%- if controller_name != 'sessions' %>
  <%= link_to "登録済みの方", new_session_path(resource_name), :class => "btn btn-secondary btn-block col-4" %><br />
<% end %>

<%- if devise_mapping.registerable? && controller_name != 'registrations' %>
  <%= link_to "登録がまだの方", new_registration_path(resource_name), :class => "btn btn-secondary btn-block col-4" %><br />
<% end %>
